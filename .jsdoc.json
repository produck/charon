{
  "source": {
    "include": ["./packages/charon/src", "./packages/charon/README.md"],
    "exclude": ["./docs", "./dist", "./node_modules"],
    "includePattern": ".js$"
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "./node_modules/minami"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "sort": false
  }
}
